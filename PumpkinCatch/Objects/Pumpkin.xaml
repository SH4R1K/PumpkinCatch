<UserControl x:Class="PumpkinCatch.Objects.Pumpkin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PumpkinCatch.Objects"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="150">
    <Rectangle x:Name="hitBoxPumpkin" Height="100" Width="100">
        <Rectangle.Fill>
            <ImageBrush ImageSource="/Assets/pumpkin.png" Stretch="UniformToFill"/>
        </Rectangle.Fill>
    </Rectangle>
</UserControl>
